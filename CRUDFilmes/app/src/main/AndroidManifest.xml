<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="semestre1.nathan.filipe.hoepers.otes06.com.example.udesc.crud">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:usesCleartextTraffic="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="semestre1.nathan.filipe.hoepers.otes06.com.example.udesc.views.Filmes"
            android:label="@string/title_activity_filmes" />
        <activity
            android:name="semestre1.nathan.filipe.hoepers.otes06.com.example.udesc.views.Directors"
            android:label="@string/title_activity_directors"></activity>
    </application>

</manifest>